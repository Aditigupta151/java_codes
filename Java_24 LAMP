class Lamp {
    boolean isOn;

    void turnOn() {
        isOn = true;
        System.out.println("Lamp is On");
    }

    void turnOff() {
        isOn = false;
        System.out.println("Lamp is Off");
    }
}

public class Main {
    public static void main(String[] args) {
        Lamp l1 = new Lamp();
        l1.turnOn();
        l1.turnOff();
    }
}
